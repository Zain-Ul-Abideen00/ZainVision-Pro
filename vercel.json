{
    "version": 2,
    "builds": [
        {
            "src": "backend/main.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.9",
                "maxDuration": 60
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/main.py"
        }
    ],
    "env": {
        "PYTHONUNBUFFERED": "1",
        "PYTHONPATH": "/var/task"
    }
}
